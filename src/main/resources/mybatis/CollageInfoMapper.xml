<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zr.common.mapper.CollageInfoMapper">
    <select id="selectByCollageId" parameterType="com.zr.common.entity.CollageInfo" resultType="com.zr.common.entity.CollageInfo">
        SELECT *
        FROM CollageInfo
        WHERE  collageid=#{collageid}
    </select>

</mapper>